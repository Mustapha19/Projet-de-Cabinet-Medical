            <Font name="Ubuntu Condensed" size="23.0" />
         </font>
      </JFXTextField>
      <TableView fx:id="medicament_list" layoutX="279.0" layoutY="82.0" prefHeight="483.0" prefWidth="1011.0">
         <columns>
            <TableColumn fx:id="nom_Internat" prefWidth="75.0" text="nom_Internat" />
            <TableColumn fx:id="nom_de_marque" prefWidth="75.0" text="nom_de_marque" />
            <TableColumn fx:id="forme" prefWidth="75.0" text="Forme" />
            <TableColumn fx:id="dosage" prefWidth="75.0" text="Dosage" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <JFXButton fx:id="sauvegarder" buttonType="RAISED" disable="true" layoutX="229.0" layoutY="585.0" onAction="#souvgarder" ripplerFill="#034e51" text="Sauvegarder" textAlignment="CENTER" textFill="#fcf5f5">
         <font>
            <Font name="Ubuntu Condensed" size="25.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="fermer" buttonType="RAISED" cancelButton="true" layoutX="19.0" layoutY="585.0" onAction="#fermer" prefHeight="48.0" prefWidth="150.0" ripplerFill="#034e51" text="Fermer" textAlignment="CENTER" textFill="#fcf5f5">
         <font>
            <Font name="Ubuntu Condensed" size="25.0" />
         </font>
      </JFXButton>
