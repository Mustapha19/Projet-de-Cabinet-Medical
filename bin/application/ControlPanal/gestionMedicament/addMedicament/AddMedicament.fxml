<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="add_medicament" prefHeight="650.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControlPanal.gestionMedicament.addMedicament.AddMedicamentController">
   <children>
      <Label layoutX="32.0" layoutY="14.0" text="Ajouter Un Medicament" textAlignment="JUSTIFY" textFill="#006064">
         <font>
            <Font name="Ubuntu Condensed" size="45.0" />
         </font>
      </Label>
      <JFXTextField fx:id="nom" focusColor="#006064" labelFloat="true" layoutX="12.0" layoutY="143.0" prefWidth="240.0" promptText="Nom de Scientifique" unFocusColor="#00838f">
         <font>
            <Font name="Ubuntu Condensed" size="23.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="marque" focusColor="#006064" labelFloat="true" layoutX="12.0" layoutY="234.0" prefWidth="240.0" promptText="Nom de marque" unFocusColor="#00838f">
         <font>
            <Font name="Ubuntu Condensed" size="23.0" />
         </font>
      </JFXTextField>
      <JFXComboBox fx:id="forme_medicament" focusColor="#006064" labelFloat="true" layoutX="12.0" layoutY="318.0" prefHeight="37.0" prefWidth="240.0" promptText="Forme" unFocusColor="#00838f" />
      <JFXTextField fx:id="dosage_medicament" focusColor="#006064" labelFloat="true" layoutX="12.0" layoutY="413.0" prefWidth="240.0" promptText="Dosage" unFocusColor="#00838f">
